    "electron": "^22.3.4",
    "electron-builder": "^23.6.0",
    "electron-vite": "^1.0.21",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "prettier": "^2.8.7",
